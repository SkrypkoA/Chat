%h1.brand-name.center-block
  = @category.name

%ul.bxslider
  - @pictures.each_with_index do |picture, index|
    %li
      = link_to image_tag(picture.location_url, { class: "picture center-block vertical-center", id: index }), category_picture_path(picture.category, picture)
.row.category-dropdown.dropup
  .btn-group.pull-right
    %button.btn.btn-default.dropdown-toggle.glyphicon.glyphicon-menu-hamburger{ role: 'button', type: "button", 'data-toggle': "dropdown", 'aria-haspopup':"true", 'aria-expanded': false }
    %ul.dropdown-menu
      %li
        = render "pictures/upload"
